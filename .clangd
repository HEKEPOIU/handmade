
CompileFlags:
  Add: [
    -DHANDMADE_SLOW=1,
    -DHANDMADE_INTERNAL=1,
    -DHANDMADE_WIN32=1,
    /std:c++20,
    /Zi,
    /FC,
    -Wall,
    -Wextra,
    -Wno-unused-variable,
    -Wno-unused-parameter,
    -Wno-missing-designated-field-initializers,
    -Wno-cast-function-type-mismatch,
    -Wno-null-dereference,
    -Wno-unused-function
  ]
